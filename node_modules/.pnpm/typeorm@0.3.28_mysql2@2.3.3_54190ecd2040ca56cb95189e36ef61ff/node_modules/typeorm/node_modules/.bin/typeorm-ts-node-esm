#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/Nucl3/Desktop/ServerServeros/node_modules/.pnpm/typeorm@0.3.28_mysql2@2.3.3_54190ecd2040ca56cb95189e36ef61ff/node_modules/typeorm/node_modules:/mnt/c/Users/Nucl3/Desktop/ServerServeros/node_modules/.pnpm/typeorm@0.3.28_mysql2@2.3.3_54190ecd2040ca56cb95189e36ef61ff/node_modules:/mnt/c/Users/Nucl3/Desktop/ServerServeros/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/Nucl3/Desktop/ServerServeros/node_modules/.pnpm/typeorm@0.3.28_mysql2@2.3.3_54190ecd2040ca56cb95189e36ef61ff/node_modules/typeorm/node_modules:/mnt/c/Users/Nucl3/Desktop/ServerServeros/node_modules/.pnpm/typeorm@0.3.28_mysql2@2.3.3_54190ecd2040ca56cb95189e36ef61ff/node_modules:/mnt/c/Users/Nucl3/Desktop/ServerServeros/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../cli-ts-node-esm.js" "$@"
else
  exec node  "$basedir/../../cli-ts-node-esm.js" "$@"
fi
