{
  "module": {
    "type": "commonjs"
  },
  "jsc": {
    "target": "es2019",
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "keepClassNames": true
  },
  "sourceMaps": true
}
